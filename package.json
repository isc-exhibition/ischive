    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "20.9.4",
    "@types/react": "18.2.38",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "gh-pages": "^6.1.0",
    "postcss": "^8",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "tailwindcss": "^3.3.0",
    "typescript": "5.3.2"
  }
}
